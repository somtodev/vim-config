source ~/.vim/my_settings.vim
source ~/.vim/customization.vim
source ~/.vim/coc_settings.vim

syntax enable

set number
set smartindent
set autoindent
set tabstop=4
set shiftwidth=4
set expandtab
set ruler
set hlsearch

" set backup
" autocmd! bufwritepost .vimrc source % " auto update latest vimrc settings "
